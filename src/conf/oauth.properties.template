# Google OAuth2 Configuration Template
# Copy this file to oauth.properties and fill in your actual credentials
# NEVER commit oauth.properties to git!

# Google OAuth2 credentials from Google Cloud Console
google.oauth.client.id=your-google-client-id.apps.googleusercontent.com
google.oauth.client.secret=your-google-client-secret
google.oauth.redirect.uri=http://localhost:8080/clubs/oauth2callback

# Base URL for your application
app.base.url=http://localhost:8080
